          "Suppliers"
        ],
        "responses": {
          "200": {
            "description": "List of basic supplier details",
            "content": {
              "application/xml": {
                "schema": {
                  "$ref": "#/components/schemas/ArrayOfSupplier"
                },
                "examples": {
                  "GetSuppliers": {
                    "value": "<ArrayOfSupplier xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\" xmlns:xsd=\"http://www.w3.org/2001/XMLSchema\">\n  <Supplier>\n      <Name>Sodales At Corp.</Name>\n      <Code>SUP001</Code>\n      <SupplierApprovalStatus>New</SupplierApprovalStatus>\n  </Supplier>\n  <Supplier>\n      <Name>Nulla Magna Malesuada Company</Name>\n      <SupplierApprovalStatus>New</SupplierApprovalStatus>\n  </Supplier>\n    </ArrayOfSupplier>"
