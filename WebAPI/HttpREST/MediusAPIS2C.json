    "/S2CAPI/v1/contracts/{ContractNumber}": {
      "put": {
        "summary": "Update the contract",
        "description": "Update the contract by passing in list of properties",
        "operationId": "PutContract",
        "parameters": [
          {
            "name": "ContractNumber",
            "in": "path",
            "required": true,
            "description": "The medius contract reference number",
            "example": "C01192",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "description": "The updated contract details",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ContractUpdate"
              },
              "examples": {
                "ContractUpdate": {
                  "value": "{\n  \"AdditionalProperties\": [\n  {\n  \"PropertyId\": \"EstimatedContractValue\",\n  \"PropertyValue\": \"Bespoke value\"\n  },\n  {\n  \"PropertyId\": \"SAPIntegrationStatus\",\n  \"PropertyValue\": \"Success\"\n  }\n  ]\n}\n  "
                }
              }
            },
            "application/xml": {
              "schema": {
                "$ref": "#/components/schemas/ContractUpdate"
              },
              "examples": {
                "ContractUpdate": {
                  "value": "<?xml version=\"1.0\" encoding=\"UTF-8\" ?>\n<root>\n  <AdditionalProperties>\n    <PropertyId>EstimatedContractValue</PropertyId>\n    <PropertyValue>Bespoke value</PropertyValue>\n  </AdditionalProperties>\n  <AdditionalProperties>\n    <PropertyId>SAPIntegrationStatus</PropertyId>\n    <PropertyValue>Success</PropertyValue>\n  </AdditionalProperties>\n</root>"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Contract details document"
          },
          "default": {
            "description": "Error response",
            "content": {
              "application/xml": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        },
        "tags": [
          "Contracts"
        ]
      }
    }
